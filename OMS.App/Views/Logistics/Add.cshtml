@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="main">
    <div class="main_edit">
        <form id="subform" method="post">
            <table class="table">
                <tbody>
                    <tr>
                        <th>
                            <span class="color_danger">*</span>@ViewBag.LanguagePack["logistics_edit_name"]：
                        </th>
                        <td>
                            <input name="ExpressName" type="text" class="easyui-textbox input-large" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            <span class="color_danger">*</span>@ViewBag.LanguagePack["logistics_edit_code"]：
                        </th>
                        <td>
                            <input name="Code" type="text" class="easyui-textbox input-large" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @ViewBag.LanguagePack["logistics_edit_url"]：
                        </th>
                        <td>
                            <input name="ExpressUrl" type="text" class="easyui-textbox input-xxlarge" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @ViewBag.LanguagePack["logistics_edit_api_url"]：
                        </th>
                        <td>
                            <input name="APIUrl" type="text" class="easyui-textbox input-xlarge" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @ViewBag.LanguagePack["logistics_edit_api_client_id"]：
                        </th>
                        <td>
                            <input name="APIClientID" type="text" class="easyui-textbox input-xlarge" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @ViewBag.LanguagePack["logistics_edit_api_client_secret"]：
                        </th>
                        <td>
                            <input name="AppClientSecret" type="text" class="easyui-textbox input-xlarge" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @ViewBag.LanguagePack["logistics_edit_api_access_token"]：
                        </th>
                        <td>
                            <input name="AccessToken" type="text" class="easyui-textbox input-xxlarge" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @ViewBag.LanguagePack["logistics_edit_api_access_token_expires"]：
                        </th>
                        <td>
                            <input name="AccessTokenExpires" type="text" class="input-medium Wdate" onclick="WdatePicker({dateFmt:'yyyy-MM-dd'})" />
                        </td>
                    </tr>
                    <tr>
                        <th>
                            <span class="color_danger">*</span>@ViewBag.LanguagePack["logistics_edit_isused"]：
                        </th>
                        <td>
                            <input id="Status_1" name="Status" type="radio" value="1" checked="checked"><label for="Status_1">@ViewBag.LanguagePack["common_yes"]</label>
                            <input id="Status_2" name="Status" type="radio" value="0"><label for="Status_2">@ViewBag.LanguagePack["common_no"]</label>
                        </td>
                    </tr>
                </tbody>
            </table>
        </form>
    </div>
</div>
@section scripts {
    @Scripts.Render("~/Content/js-library/my97datepicker/4.7.2/WdatePicker.js")
}